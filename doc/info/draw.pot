"        ellipse(0,0,1,1,0,360),\n"
"        transparent=true,\n"
"        color = blue,\n"
"        line_width = 4,\n"
"        ellipse(0,0,2,1/2,0,360),\n"
"        proportional_axes = xy) $\n"
"@end example\n"
