"             points([[1,2],[5,2],[9,2]]),\n"
"             point_type = asterisk,\n"
"             points([[1,3],[5,3],[9,3]]),\n"
"             point_type = filled_diamant,\n"
"             points([[1,4],[5,4],[9,4]]),\n"
"             point_type = 5,\n"
"             points([[1,5],[5,5],[9,5]]),\n"
"             point_type = 6,\n"
"             points([[1,6],[5,6],[9,6]]),\n"
