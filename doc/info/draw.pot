#: draw.texi:1929
msgid ""
"@example\n"
"(%i1) draw2d(\n"
"        key_pos = top_left,\n"
"        key   = \\"x\\",\n"
"        explicit(x,  x,0,10),\n"
"        color= red,\n"
"        key   = \\"x squared\\",\n"
"        explicit(x^2,x,0,10))$\n"
"(%i3) draw3d(\n"
"        key_pos = center,\n"
"        key   = \\"x\\",\n"
"        explicit(x+y,x,0,10,y,0,10),\n"
