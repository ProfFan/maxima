"          allocation = [[0, 0],[1, 1/4]],\n"
"          explicit(x^2,x,-1,1)),\n"
"        gr3d(\n"
"          allocation = [[0, 1/4],[1, 3/4]],\n"
"          explicit(x^2+y^2,x,-1,1,y,-1,1) ))$\n"
"@end example\n"
"@figure{draw_allocation2}\n"
