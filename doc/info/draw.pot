"             color = red,\n"
"             explicit(0.5 + x^2,x,-1,1),\n"
"             color = blue,\n"
"             explicit(1 + x^2,x,-1,1),\n"
"             color = light_blue,\n"
"             explicit(1.5 + x^2,x,-1,1),\n"
"             color = \\"#23ab0f\\",\n"
"             label([\\"This is a label\\",0,1.2])  )$\n"
"@end example\n"
"@figure{draw_color}\n"
"@example\n"
"(%i1) draw2d(\n"
"             line_width=50,\n"
