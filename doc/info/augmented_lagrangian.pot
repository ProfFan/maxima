# augmented_lagrangian.texi
msgid ""
"Examples:\n"
msgstr ""

# augmented_lagrangian.texi
msgid ""
"@c ===beg===\n"
"@c load (lbfgs);\n"
"@c load (augmented_lagrangian);\n"
"@c FOM: x^2 + 2*y^2;\n"
"@c xx: [x, y];\n"
"@c C: [x + y - 1];\n"
"@c yy: [1, 1];\n"
"@c augmented_lagrangian_method(FOM, xx, C, yy, iprint=[-1,0]);\n"
"@c ===end===\n"
"@example\n"
"(%i1) load (lbfgs);\n"
"(%o1)     /maxima/share/lbfgs/lbfgs.mac\n"
"(%i2) load (augmented_lagrangian);\n"
"(%o2) \n"
"   /maxima/share/contrib/augmented_lagrangian.mac\n"
"(%i3) FOM: x^2 + 2*y^2;\n"
"                               2    2\n"
"(%o3)                       2 y  + x\n"
"(%i4) xx: [x, y];\n"
"(%o4)                        [x, y]\n"
"(%i5) C: [x + y - 1];\n"
"(%o5)                      [y + x - 1]\n"
"(%i6) yy: [1, 1];\n"
"(%o6)                        [1, 1]\n"
"(%i7) augmented_lagrangian_method(FOM, xx, C, yy, iprint=[-1,0]);\n"
"(%o7) [[x = 0.66665984108002, y = 0.33334027245545],\n"
"                                 %lambda = [- 1.333337940892525]]\n"
"@end example\n"
msgstr ""

# augmented_lagrangian.texi
msgid ""
"Same example as before, but this time the gradient is supplied as an argument.\n"
msgstr ""

# augmented_lagrangian.texi
msgid ""
"@c ===beg===\n"
"@c load (lbfgs)$\n"
"@c load (augmented_lagrangian)$\n"
"@c FOM: x^2 + 2*y^2;\n"
"@c xx: [x, y];\n"
"@c grad : [2*x, 4*y];\n"
"@c C: [x + y - 1];\n"
"@c yy: [1, 1];\n"
"@c augmented_lagrangian_method ([FOM, grad], xx, C, yy, \n"
"@c                              iprint = [-1, 0]);\n"
"@c ===end===\n"
"@example\n"
"(%i1) load (lbfgs)$\n"
"(%i2) load (augmented_lagrangian)$\n"
"(%i3) FOM: x^2 + 2*y^2;\n"
"                               2    2\n"
"(%o3)                       2 y  + x\n"
"(%i4) FOM: x^2 + 2*y^2;\n"
"                               2    2\n"
"(%o4)                       2 y  + x\n"
"(%i5) xx: [x, y];\n"
"(%o5)                        [x, y]\n"
"(%i6) grad : [2*x, 4*y];\n"
"(%o6)                      [2 x, 4 y]\n"
"(%i7) C: [x + y - 1];\n"
"(%o7)                      [y + x - 1]\n"
"(%i8) yy: [1, 1];\n"
"(%o8)                        [1, 1]\n"
"(%i9) augmented_lagrangian_method ([FOM, grad], xx, C, yy,\n"
"                                   iprint = [-1, 0]);\n"
"(%o9) [[x = 0.666659841080025, y = .3333402724554462], \n"
"                                 %lambda = [- 1.333337940892543]]\n"
"@end example\n"
msgstr ""

