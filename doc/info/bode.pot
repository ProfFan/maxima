# bode.texi
msgid ""
"@node Functions and Variables for bode,  , bode-pkg, bode-pkg\n"
"@section Functions and Variables for bode\n"
"@anchor{bode_gain}\n"
"@deffn {Function} bode_gain (@var{H}, @var{range}, ...@var{plot_opts}...)\n"
"Function to draw Bode gain plots.\n"
msgstr ""

# bode.texi
msgid ""
"Examples (1 through 7 from \n"
"@example\n"
"@url{http://www.swarthmore.edu/NatSci/echeeve1/Ref/Bode/BodeHow.html},\n"
"@end example\n"
"8 from Ron Crummett):\n"
"@example\n"
"(%i1) load(\"bode\")$\n"
msgstr ""

# bode.texi
msgid ""
"(%i2) H1 (s) := 100 * (1 + s) / ((s + 10) * (s + 100))$\n"
msgstr ""

# bode.texi
msgid ""
"(%i3) bode_gain (H1 (s), [w, 1/1000, 1000])$\n"
msgstr ""

# bode.texi
msgid ""
"(%i4) H2 (s) := 1 / (1 + s/omega0)$\n"
msgstr ""

# bode.texi
msgid ""
"(%i5) bode_gain (H2 (s), [w, 1/1000, 1000]), omega0 = 10$\n"
msgstr ""

# bode.texi
msgid ""
"(%i6) H3 (s) := 1 / (1 + s/omega0)^2$\n"
msgstr ""

# bode.texi
msgid ""
"(%i7) bode_gain (H3 (s), [w, 1/1000, 1000]), omega0 = 10$\n"
msgstr ""

# bode.texi
msgid ""
"(%i8) H4 (s) := 1 + s/omega0$\n"
msgstr ""

# bode.texi
msgid ""
"(%i9) bode_gain (H4 (s), [w, 1/1000, 1000]), omega0 = 10$\n"
msgstr ""

# bode.texi
msgid ""
"(%i10) H5 (s) := 1/s$\n"
msgstr ""

# bode.texi
msgid ""
"(%i11) bode_gain (H5 (s), [w, 1/1000, 1000])$\n"
msgstr ""

# bode.texi
msgid ""
"(%i12) H6 (s) := 1/((s/omega0)^2 + 2 * zeta * (s/omega0) + 1)$\n"
msgstr ""

# bode.texi
msgid ""
"(%i13) bode_gain (H6 (s), [w, 1/1000, 1000]), \n"
"                  omega0 = 10, zeta = 1/10$\n"
msgstr ""

# bode.texi
msgid ""
"(%i14) H7 (s) := (s/omega0)^2 + 2 * zeta * (s/omega0) + 1$\n"
msgstr ""

# bode.texi
msgid ""
"(%i15) bode_gain (H7 (s), [w, 1/1000, 1000]),\n"
"                  omega0 = 10, zeta = 1/10$\n"
msgstr ""

# bode.texi
msgid ""
"(%i16) H8 (s) := 0.5 / (0.0001 * s^3 + 0.002 * s^2 + 0.01 * s)$\n"
msgstr ""

# bode.texi
msgid ""
"(%i17) bode_gain (H8 (s), [w, 1/1000, 1000])$\n"
"@end example\n"
msgstr ""

# bode.texi
msgid ""
"To use this function write first @code{load(\"bode\")}. See also @mrefdot{bode_phase}\n"
msgstr ""

# bode.texi
msgid ""
"@opencatbox\n"
"@category{Plotting} @category{Package bode}\n"
"@closecatbox\n"
msgstr ""

# bode.texi
msgid ""
"@end deffn\n"
msgstr ""

