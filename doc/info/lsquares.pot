"                            [ 1  1  ]\n"
"                            [       ]\n"
"                            [    7  ]\n"
"                            [ 2  -  ]\n"
"                            [    4  ]\n"
"                            [       ]\n"
"(%o2)                       [    11 ]\n"
"                            [ 3  -- ]\n"
"                            [    4  ]\n"
"                            [       ]\n"
"                            [    13 ]\n"
"                            [ 4  -- ]\n"
"                            [    4  ]\n"
"@group\n"
"(%i3) lsquares_estimates (\n"
