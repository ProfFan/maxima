#: lsquares.texi:364
msgid ""
"@anchor{lsquares_mse}\n"
"@deffn {Function} lsquares_mse (@var{D}, @var{x}, @var{e})\n"
msgstr ""

#: lsquares.texi:367
msgid ""
"Returns the mean square error (MSE), a summation expression, for the equation @var{e}\n"
"in the variables @var{x}, with data @var{D}.\n"
msgstr ""

#: lsquares.texi:369
msgid ""
"The MSE is defined as:\n"
msgstr ""

#: lsquares.texi:384
msgid ""
"@tex\n"
"$${1 \over n} \, \sum_{i=1}^n \left[{\rm lhs}\left(e_i\right) - {\rm rhs}\left(e_i\right)\right]^2,$$\n"
"@end tex\n"
"@ifnottex\n"
"@example\n"
"                    n\n"
"                   ====\n"
"               1   \                        2\n"
"               -    >    (lhs(e ) - rhs(e ))\n"
"               n   /           i         i\n"
"                   ====\n"
"                   i = 1\n"
"@end example\n"
"@end ifnottex\n"
msgstr ""

#: lsquares.texi:387
msgid ""
"where @var{n} is the number of data and @code{@var{e}[i]} is the equation @var{e}\n"
"evaluated with the variables in @var{x} assigned values from the @code{i}-th datum, @code{@var{D}[i]}.\n"
msgstr ""

#: lsquares.texi:389
msgid ""
"@code{load(lsquares)} loads this function.\n"
@c Occurrences of paragraphs with this contents up to now: 2
msgstr ""

#: lsquares.texi:391
msgid ""
"Example:\n"
@c Occurrences of paragraphs with this contents up to now: 2
msgstr ""

#: lsquares.texi:487
msgid ""
"@c ===beg===\n"
"@c load (lsquares)$\n"
"@c M : matrix (\n"
"@c          [1,1,1], [3/2,1,2], [9/4,2,1], [3,2,2], [2,2,1]);\n"
"@c mse : lsquares_mse (M, [z, x, y], (z + D)^2 = A*x + B*y + C);\n"
"@c diff (mse, D);\n"
"@c ''mse, nouns;\n"
"@c ===end===\n"
"@example\n"
"(%i1) load (lsquares)$\n"
"(%i2) M : matrix (\n"
"         [1,1,1], [3/2,1,2], [9/4,2,1], [3,2,2], [2,2,1]);\n"
"                           [ 1  1  1 ]\n"
"                           [         ]\n"
"                           [ 3       ]\n"
"                           [ -  1  2 ]\n"
"                           [ 2       ]\n"
"                           [         ]\n"
"(%o2)                      [ 9       ]\n"
"                           [ -  2  1 ]\n"
"                           [ 4       ]\n"
"                           [         ]\n"
"                           [ 3  2  2 ]\n"
"                           [         ]\n"
"                           [ 2  2  1 ]\n"
"(%i3) mse : lsquares_mse (M, [z, x, y], (z + D)^2 = A*x + B*y + C);\n"
"         5\n"
"        ====\n"
"        \                                         2     2\n"
"         >    ((- B M    ) - A M     + (M     + D)  - C)\n"
"        /            i, 3       i, 2     i, 1\n"
"        ====\n"
"        i = 1\n"
"(%o3)   -------------------------------------------------\n"
"                                5\n"
"(%i4) diff (mse, D);\n"
"(%o4) \n"
"      5\n"
"     ====\n"
"     \                                                     2\n"
"   4  >    (M     + D) ((- B M    ) - A M     + (M     + D)  - C)\n"
"     /       i, 1             i, 3       i, 2     i, 1\n"
"     ====\n"
"     i = 1\n"
"   --------------------------------------------------------------\n"
"                                 5\n"
"@group\n"
"(%i5) ''mse, nouns;\n"
"               2                 2         9 2               2\n"
"(%o5) (((D + 3)  - C - 2 B - 2 A)  + ((D + -)  - C - B - 2 A)\n"
"                                           4\n"
"           2               2         3 2               2\n"
" + ((D + 2)  - C - B - 2 A)  + ((D + -)  - C - 2 B - A)\n"
"                                     2\n"
"           2             2\n"
" + ((D + 1)  - C - B - A) )/5\n"
"@end group\n"
"@end example\n"
"@example\n"
"(%i3) mse : lsquares_mse (M, [z, x, y], (z + D)^2 = A*x + B*y + C);\n"
"           5\n"
"          ====\n"
"          \                 2                         2\n"
"           >    ((D + M    )  - C - M     B - M     A)\n"
"          /            i, 1          i, 3      i, 2\n"
"          ====\n"
"          i = 1\n"
"(%o3)     ---------------------------------------------\n"
"                                5\n"
"@end example\n"
"@example\n"
"(%i4) diff (mse, D);\n"
"         5\n"
"        ====\n"
"        \                             2\n"
"      4  >    (D + M    ) ((D + M    )  - C - M     B - M     A)\n"
"        /           i, 1         i, 1          i, 3      i, 2\n"
"        ====\n"
"        i = 1\n"
"(%o4) ----------------------------------------------------------\n"
"                                  5\n"
"@end example\n"
"@example\n"
"(%i5) ''mse, nouns;\n"
"@group\n"
"               2                 2         9 2               2\n"
"(%o5) (((D + 3)  - C - 2 B - 2 A)  + ((D + -)  - C - B - 2 A)\n"
"                                           4\n"
"           2               2         3 2               2\n"
" + ((D + 2)  - C - B - 2 A)  + ((D + -)  - C - 2 B - A)\n"
"                                     2\n"
"           2             2\n"
" + ((D + 1)  - C - B - A) )/5\n"
"@end group\n"
"@end example\n"
msgstr ""

