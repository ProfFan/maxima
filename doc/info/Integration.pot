#: Integration.texi:1690
msgid ""
"@table @code\n"
"@item 1\n"
"@ifnottex\n"
"@math{w(x) = (x - a)^alpha (b - x)^beta}\n"
"@end ifnottex\n"
"@tex\n"
"$w\left(x\right) = \left(x - a\right)^\alpha \, \left(b - x\right)^\beta$\n"
"@end tex\n"
"@item 2\n"
"@ifnottex\n"
"@math{w(x) = (x - a)^alpha (b - x)^beta log(x - a)}\n"
"@end ifnottex\n"
"@tex\n"
"$w\left(x\right) = \left(x - a\right)^\alpha \, \left(b - x\right)^\beta \, \log\left(x - a\right)$\n"
"@end tex\n"
"@item 3\n"
"@ifnottex\n"
"@math{w(x) = (x - a)^alpha (b - x)^beta log(b - x)}\n"
"@end ifnottex\n"
"@tex\n"
"$w\left(x\right) = \left(x - a\right)^\alpha \, \left(b - x\right)^\beta \, \log\left(b - x\right)$\n"
"@end tex\n"
"@item 4\n"
"@ifnottex\n"
"@math{w(x) = (x - a)^alpha (b - x)^beta log(x - a) log(b - x)}\n"
"@end ifnottex\n"
"@tex\n"
