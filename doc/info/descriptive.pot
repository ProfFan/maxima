# descriptive.texi
msgid ""
"@anchor{skewness}\n"
"@deffn {Function} skewness @\n"
"@fname{skewness} (@var{list}) @\n"
"@fname{skewness} (@var{matrix})\n"
msgstr ""

# descriptive.texi
msgid ""
"The skewness coefficient, defined as\n"
"@ifnottex\n"
"@example\n"
"@group\n"
"                    n\n"
"                  ====\n"
"            1     \          _ 3\n"
"           ----    >    (x - x)\n"
"              3   /       i\n"
"           n s    ====\n"
"                  i = 1\n"
"@end group\n"
"@end example\n"
