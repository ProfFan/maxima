msgstr ""

# descriptive.texi
msgid ""
"The harmonic mean, defined as\n"
"@ifnottex\n"
"@example\n"
"@group\n"
"                  n\n"
"               --------\n"
"                n\n"
"               ====\n"
"               \     1\n"
"                >    --\n"
