"@fname{kurtosis} (@var{list}) @\n"
"@fname{kurtosis} (@var{matrix})\n"
msgstr ""

# descriptive.texi
msgid ""
"The kurtosis coefficient, defined as\n"
"@ifnottex\n"
"@example\n"
"@group\n"
"                    n\n"
"                  ====\n"
"            1     \          _ 4\n"
"           ----    >    (x - x)  - 3\n"
"              4   /       i\n"
"           n s    ====\n"
"                  i = 1\n"
"@end group\n"
"@end example\n"
"@end ifnottex\n"
"@tex\n"
"$${{1\over{n s^4}}{\sum_{i=1}^{n}{(x_{i}-\bar{x})^4}}-3}$$\n"
"@end tex\n"
msgstr ""

# descriptive.texi
msgid ""
"Example:\n"
msgstr ""

# descriptive.texi
msgid ""
"@c ===beg===\n"
"@c load (descriptive)$\n"
"@c s1 : read_list (file_search (\"pidigits.data\"))$\n"
"@c kurtosis (s1), numer;\n"
"@c s2 : read_matrix (file_search (\"wind.data\"))$\n"
"@c kurtosis (s2);\n"
"@c ===end===\n"
"@example\n"
"(%i1) load (descriptive)$\n"
"(%i2) s1 : read_list (file_search (\"pidigits.data\"))$\n"
"@group\n"
"(%i3) kurtosis (s1), numer;\n"
"(%o3)                  - 1.273247946514421\n"
"@end group\n"
"(%i4) s2 : read_matrix (file_search (\"wind.data\"))$\n"
"@group\n"
"(%i5) kurtosis (s2);\n"
"(%o5) [- .2715445622195385, 0.119998784429451, \n"
"     - .4275233490482861, - .6405361979019522, - .4952382132352935]\n"
"@end group\n"
"@end example\n"
msgstr ""

