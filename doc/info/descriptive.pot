"                                     2\n"
"                3 charfun((2 <= x) and (x < 3))\n"
"              + ------------------------------- \n"
"                               4\n"
"              + charfun((0 <= x) and (x < 2))\n"
"@end group\n"
"(%i3) load (draw)$\n"
"@group\n"
"(%i4) draw2d(\n"
"        line_width = 3, grid = true,\n"
