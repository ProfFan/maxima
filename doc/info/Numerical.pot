msgid ""
"Examples:\n"
msgstr ""

#: Numerical.texi:123
msgid ""
"Real data.\n"
msgstr ""

#: Numerical.texi:146
msgid ""
"@c ===beg===\n"
"@c load (fft) $\n"
"@c fpprintprec : 4 $\n"
"@c L : [1, 2, 3, 4, -1, -2, -3, -4] $\n"
"@c L1 : inverse_fft (L);\n"
"@c L2 : fft (L1);\n"
"@c lmax (abs (L2 - L));\n"
"@c ===end===\n"
"@example\n"
"(%i1) load (fft) $\n"
"(%i2) fpprintprec : 4 $\n"
"(%i3) L : [1, 2, 3, 4, -1, -2, -3, -4] $\n"
"(%i4) L1 : inverse_fft (L);\n"
"(%o4) [0.0, 14.49 %i - .8284, 0.0, 2.485 %i + 4.828, 0.0, \n"
"                       4.828 - 2.485 %i, 0.0, - 14.49 %i - .8284]\n"
"(%i5) L2 : fft (L1);\n"
"(%o5) [1.0, 2.0 - 2.168L-19 %i, 3.0 - 7.525L-20 %i, \n"
"4.0 - 4.256L-19 %i, - 1.0, 2.168L-19 %i - 2.0, \n"
"7.525L-20 %i - 3.0, 4.256L-19 %i - 4.0]\n"
"(%i6) lmax (abs (L2 - L));\n"
"(%o6)                       3.545L-16\n"
"@end example\n"
msgstr ""

#: Numerical.texi:148
msgid ""
"Complex data.\n"
msgstr ""

#: Numerical.texi:172
msgid ""
"@c ===beg===\n"
"@c load (fft) $\n"
"@c fpprintprec : 4 $\n"
"@c L : [1, 1 + %i, 1 - %i, -1, -1, 1 - %i, 1 + %i, 1] $\n"
"@c L1 : inverse_fft (L);\n"
"@c L2 : fft (L1);\n"
"@c lmax (abs (L2 - L));\n"
"@c ===end===\n"
"@example\n"
"(%i1) load (fft) $\n"
"(%i2) fpprintprec : 4 $                 \n"
"(%i3) L : [1, 1 + %i, 1 - %i, -1, -1, 1 - %i, 1 + %i, 1] $\n"
"(%i4) L1 : inverse_fft (L);\n"
"(%o4) [4.0, 2.711L-19 %i + 4.0, 2.0 %i - 2.0, \n"
"- 2.828 %i - 2.828, 0.0, 5.421L-20 %i + 4.0, - 2.0 %i - 2.0, \n"
"2.828 %i + 2.828]\n"
"(%i5) L2 : fft (L1);\n"
"(%o5) [4.066E-20 %i + 1.0, 1.0 %i + 1.0, 1.0 - 1.0 %i, \n"
"1.55L-19 %i - 1.0, - 4.066E-20 %i - 1.0, 1.0 - 1.0 %i, \n"
"1.0 %i + 1.0, 1.0 - 7.368L-20 %i]\n"
"(%i6) lmax (abs (L2 - L));                    \n"
"(%o6)                       6.841L-17\n"
"@end example\n"
msgstr ""

