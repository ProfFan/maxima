"(%i3) array (a, fixnum, 2, 2);\n"
"(%o3)                           a\n"
"(%i4) a [1, 1] : %e;\n"
"(%o4)                          %e\n"
"(%i5) a [2, 2] : %pi;\n"
"(%o5)                          %pi\n"
"(%i6) genmatrix (a, 2, 2);\n"
"                           [ %e   0  ]\n"
"(%o6)                      [         ]\n"
"                           [ 0   %pi ]\n"
"(%i7) genmatrix (lambda ([i, j], j - i), 3, 3);\n"
"                         [  0    1   2 ]\n"
"                         [             ]\n"
"(%o7)                    [ - 1   0   1 ]\n"
"                         [             ]\n"
"                         [ - 2  - 1  0 ]\n"
"(%i8) genmatrix (B, 2, 2);\n"
"                        [ B      B     ]\n"
"                        [  1, 1   1, 2 ]\n"
"(%o8)                   [              ]\n"
