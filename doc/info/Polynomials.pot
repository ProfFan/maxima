#: Polynomials.texi:299
msgid ""
"@c ===beg===\n"
"@c coeff (c*(a + b)^3, a);\n"
"@c expand (c*(a + b)^3);\n"
"@c coeff (%, a);\n"
"@c coeff (b^3*c + 3*a*b^2*c + 3*a^2*b*c + a^3*c, (a + b)^3);\n"
"@c factor (b^3*c + 3*a*b^2*c + 3*a^2*b*c + a^3*c);\n"
"@c coeff (%, (a + b)^3);\n"
"@c ===end===\n"
"@example\n"
"@group\n"
"(%i1) coeff (c*(a + b)^3, a);\n"
"(%o1)                           0\n"
"@end group\n"
"@group\n"
"(%i2) expand (c*(a + b)^3);\n"
"                 3          2        2        3\n"
"(%o2)           b  c + 3 a b  c + 3 a  b c + a  c\n"
"@end group\n"
"@group\n"
"(%i3) coeff (%, a);\n"
"                                2\n"
"(%o3)                        3 b  c\n"
"@end group\n"
"@group\n"
"(%i4) coeff (b^3*c + 3*a*b^2*c + 3*a^2*b*c + a^3*c, (a + b)^3);\n"
"(%o4)                           0\n"
"@end group\n"
"@group\n"
"(%i5) factor (b^3*c + 3*a*b^2*c + 3*a^2*b*c + a^3*c);\n"
"                                  3\n"
"(%o5)                      (b + a)  c\n"
"@end group\n"
"@group\n"
"(%i6) coeff (%, (a + b)^3);\n"
"(%o6)                           c\n"
"@end group\n"
"@end example\n"
msgstr ""

