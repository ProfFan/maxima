"(%o2)                     [          ]\n"
"                          [ - c  - d ]\n"
"@end group\n"
"@group\n"
"(%i3) coeff (a*u - b*v = 7*u + 3*v, u);\n"
"(%o3)                         a = 7\n"
"@end group\n"
"@end example\n"
