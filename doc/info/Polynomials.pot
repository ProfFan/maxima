"@end group\n"
"@group\n"
"(%i3) g2:sin(x)/10*cos(x)/y;\n"
"                          cos(x) sin(x)\n"
"(%o3)                     -------------\n"
"                              10 y\n"
"@end group\n"
"@group\n"
"(%i4) num(g2);\n"
"(%o4)                     cos(x) sin(x)\n"
"@end group\n"
"@end example\n"
