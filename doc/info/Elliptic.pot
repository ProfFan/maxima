# Elliptic.texi
msgid ""
"@anchor{elliptic_f}\n"
"@deffn {Function} elliptic_f (@var{phi}, @var{m})\n"
"The incomplete elliptic integral of the first kind, defined as\n"
msgstr ""

# Elliptic.texi
msgid ""
"@ifhtml\n"
"@math{integrate(1/sqrt(1 - m*sin(x)^2), x, 0, phi)}\n"
"@end ifhtml\n"
"@ifinfo\n"
"@math{integrate(1/sqrt(1 - m*sin(x)^2), x, 0, phi)}\n"
"@end ifinfo\n"
msgstr ""

# Elliptic.texi
msgid ""
"@tex\n"
"$$\int_0^\phi {{d\theta}\over{\sqrt{1 - m\sin^2\theta}}}$$\n"
"@end tex\n"
msgstr ""

# Elliptic.texi
msgid ""
"See also @ref{elliptic_e} and @ref{elliptic_kc}.\n"
msgstr ""

# Elliptic.texi
msgid ""
"@opencatbox\n"
"@category{Elliptic integrals}\n"
"@closecatbox\n"
"@end deffn\n"
msgstr ""

# Elliptic.texi
msgid ""
"@anchor{elliptic_e}\n"
"@deffn {Function} elliptic_e (@var{phi}, @var{m})\n"
"The incomplete elliptic integral of the second kind, defined as\n"
msgstr ""

# Elliptic.texi
msgid ""
"@ifhtml\n"
"@math{elliptic_e(phi, m) = integrate(sqrt(1 - m*sin(x)^2), x, 0, phi)}\n"
"@end ifhtml\n"
"@ifinfo\n"
"@math{elliptic_e(phi, m) = integrate(sqrt(1 - m*sin(x)^2), x, 0, phi)}\n"
"@end ifinfo\n"
"@tex\n"
"$$\int_0^\phi \sqrt{1 - m\sin^2\theta} d\theta$$\n"
"@end tex\n"
msgstr ""

# Elliptic.texi
msgid ""
"See also @ref{elliptic_f} and @ref{elliptic_ec}.\n"
msgstr ""

# Elliptic.texi
msgid ""
"@opencatbox\n"
"@category{Elliptic integrals}\n"
"@closecatbox\n"
"@end deffn\n"
msgstr ""

# Elliptic.texi
msgid ""
"@anchor{elliptic_eu}\n"
"@deffn {Function} elliptic_eu (@var{u}, @var{m})\n"
"The incomplete elliptic integral of the second kind, defined as\n"
msgstr ""

# Elliptic.texi
msgid ""
"@ifhtml\n"
"@math{integrate(dn(v,m)^2,v,0,u) = integrate(sqrt(1-m*t^2)/sqrt(1-t^2), t, 0, tau)}\n"
msgstr ""

# Elliptic.texi
msgid ""
"where @math{tau = sn(u,m)}.\n"
"@end ifhtml\n"
"@ifinfo\n"
"@math{integrate(dn(v,m)^2,v,0,u) = integrate(sqrt(1-m*t^2)/sqrt(1-t^2), t, 0, tau)}\n"
msgstr ""

# Elliptic.texi
msgid ""
"where @math{tau = sn(u,m)}.\n"
"@end ifinfo\n"
"@tex\n"
"$$\int_0^u {\rm dn}(v, m) dv  = \int_0^\tau \sqrt{{1-m t^2}\over{1-t^2}} dt$$\n"
msgstr ""

# Elliptic.texi
msgid ""
"where $\tau = {\rm sn}(u, m)$.\n"
"@end tex\n"
msgstr ""

# Elliptic.texi
msgid ""
"This is related to @math{elliptic_e} by\n"
msgstr ""

# Elliptic.texi
msgid ""
"@ifhtml\n"
"@math{elliptic_eu(u, m) = elliptic_e(asin(sn(u,m)),m)}\n"
"@end ifhtml\n"
"@ifinfo\n"
"@math{elliptic_eu(u, m) = elliptic_e(asin(sn(u,m)),m)}\n"
"@end ifinfo\n"
"@tex\n"
"$$E(u,m) = E(\phi, m)$$\n"
msgstr ""

# Elliptic.texi
msgid ""
"where $\phi = \sin^{-1} {\rm sn}(u, m)$.\n"
"@end tex\n"
msgstr ""

# Elliptic.texi
msgid ""
"See also @ref{elliptic_e}.\n"
"@opencatbox\n"
"@category{Elliptic integrals}\n"
"@closecatbox\n"
"@end deffn\n"
msgstr ""

# Elliptic.texi
msgid ""
"@anchor{elliptic_pi}\n"
"@deffn {Function} elliptic_pi (@var{n}, @var{phi}, @var{m})\n"
"The incomplete elliptic integral of the third kind, defined as\n"
msgstr ""

# Elliptic.texi
msgid ""
"@ifhtml\n"
"@math{integrate(1/(1-n*sin(x)^2)/sqrt(1 - m*sin(x)^2), x, 0, phi)}\n"
"@end ifhtml\n"
"@ifinfo\n"
"@math{integrate(1/(1-n*sin(x)^2)/sqrt(1 - m*sin(x)^2), x, 0, phi)}\n"
"@end ifinfo\n"
"@tex\n"
"$$\int_0^\phi {{d\theta}\over{(1-n\sin^2 \theta)\sqrt{1 - m\sin^2\theta}}}$$\n"
"@end tex\n"
msgstr ""

# Elliptic.texi
msgid ""
"@opencatbox\n"
"@category{Elliptic integrals}\n"
"@closecatbox\n"
"@end deffn\n"
msgstr ""

# Elliptic.texi
msgid ""
"@anchor{elliptic_kc}\n"
"@deffn {Function} elliptic_kc (@var{m})\n"
"The complete elliptic integral of the first kind, defined as\n"
msgstr ""

# Elliptic.texi
msgid ""
"@ifhtml\n"
"@math{integrate(1/sqrt(1 - m*sin(x)^2), x, 0, %pi/2)}\n"
"@end ifhtml\n"
"@ifinfo\n"
"@math{integrate(1/sqrt(1 - m*sin(x)^2), x, 0, %pi/2)}\n"
"@end ifinfo\n"
msgstr ""

# Elliptic.texi
msgid ""
"@tex\n"
"$$\int_0^{{\pi}\over{2}} {{d\theta}\over{\sqrt{1 - m\sin^2\theta}}}$$\n"
"@end tex\n"
"For certain values of @math{m}, the value of the integral is known in\n"
"terms of @math{Gamma} functions.  Use @code{makegamma} to evaluate them.\n"
msgstr ""

# Elliptic.texi
msgid ""
"@opencatbox\n"
"@category{Elliptic integrals}\n"
"@closecatbox\n"
"@end deffn\n"
msgstr ""

# Elliptic.texi
msgid ""
"@anchor{elliptic_ec}\n"
"@deffn {Function} elliptic_ec (@var{m})\n"
