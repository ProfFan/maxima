"(%o6)                                done\n"
"@end group\n"
"@group\n"
"(%i7) N;\n"
"                            1800000000000   %in cg\n"
"(%o7)                      (-------------) (------)\n"
"                                 127            2\n"
"                                            %min\n"
"@end group\n"
"@group\n"
"(%i8) dyn;\n"
"                               18000000   %in cg\n"
"(%o8)                         (--------) (------)\n"
"                                 127          2\n"
"                                          %min\n"
"@end group\n"
"@group\n"
"(%i9) g;\n"
"(%o9)                             (100) (cg)\n"
"@end group\n"
"@group\n"
"(%i10) centigram*inch/minutes^2;\n"
"                                    %in cg\n"
"(%o10)                              ------\n"
"                                        2\n"
"                                    %min\n"
"@end group\n"
"@end example\n"
msgstr ""

