#. (itstool) path: texifile/paragraph
#: celine.xml:32
#, no-wrap
msgid ""
"@c ===beg===\n"
"@c load(\"integer_sequence\")$\n"
"@c load(\"opsubst\")$\n"
"@c load(\"to_poly_solve\")$\n"
"@c load(\"celine\")$\n"
"@c celine(n!,n,k,1,0);\n"
"@c ===end===\n"
"@example\n"
"(%i1) load(\"integer_sequence\")$\n"
"(%i2) load(\"opsubst\")$\n"
"(%i3) load(\"to_poly_solve\")$\n"
"(%i4) load(\"celine\")$\n"
"@group\n"
"(%i5) celine(n!,n,k,1,0);\n"
"(%o5)       @{fff(n + 1, k) - n fff(n, k) - fff(n, k)@}\n"
"@end group\n"
"@end example\n"
msgstr ""

#. (itstool) path: texifile/paragraph
#: celine.xml:50
#, no-wrap
