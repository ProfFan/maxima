"@end deffn\n"
msgstr ""

# operatingsystem.texi
msgid ""
"@anchor{rename_file}\n"
"@deffn {Function} rename_file (@var{file1}, @var{file2})\n"
"renames file @var{file1} to @var{file2}\n"
"@end deffn\n"
msgstr ""

# operatingsystem.texi
