msgid ""
"@anchor{mkdir}\n"
"@deffn {Function} mkdir (@var{dir})\n"
"Create directory @var{dir}\n"
"@end deffn\n"
msgstr ""

# operatingsystem.texi
msgid ""
"@anchor{rmdir}\n"
"@deffn {Function} rmdir (@var{dir})\n"
"remove directory @var{dir}\n"
"@end deffn\n"
msgstr ""

